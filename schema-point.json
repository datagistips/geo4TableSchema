   "fields": [
      {
         "name": "fid",
         "title": "Identifier",
         "description": "Identifier for the entity",
         "example": "1",
         "type": "integer",
         "constraints": {
            "required": true,
            "unique": true
         }
      },
      {
         "name": "label",
         "title": "Label",
         "description": "Label.",
         "example": "My object",
         "type": "string",
         "constraints": {
            "required": false
         }
      },
      {
         "name": "_geom",
         "title": "Point geometry",
         "description": "Point geometry",
         "type": "wkt",
         "geomtype": "point",
         "crs": "EPSG:4326",
         "horizontalAccuracy": 5,
         "constraints": {
            "required": true,
            "unique": true,
            "overlaps": false,
            "bounds": [
               4.9283,
               43.0756,
               7.6412,
               45.0923
            ]
         }
      }
   ],
   "missingValues": [
      "",
      "NA",
      "NaN",
      "N/A"
   ],
   "primaryKey": "fid"
}